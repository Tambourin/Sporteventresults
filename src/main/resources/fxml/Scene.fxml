<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox id="VBox" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sportevent.sporteventresults.FXMLController">
    <children>
        <TextField fx:id="first_name" promptText="Etunimi" />
        <TextField fx:id="last_name" promptText="Sukunimi"> </TextField>
        <TextField fx:id="club_name" promptText="Seura"> </TextField>       
        <Button fx:id="button" onAction="#handleButtonAction" text="Lisää osanottaja" />
        <Label fx:id="label" minHeight="16" minWidth="69" />
      <TableView fx:id="participantTable" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="firstNameColumn" prefWidth="75.0" text="Etunimi" />
          <TableColumn fx:id="lastNameColumn" prefWidth="75.0" text="Sukunimi" />
        </columns>
      </TableView>
    </children>
</VBox>
